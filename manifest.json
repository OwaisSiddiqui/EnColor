{
    "manifest_version": 3,
    "name": "EnColor",
    "description": "EnColor - Chrome Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": [
                "https://*/*"
            ]
        }
    ],
    "permissions": ["activeTab"]
  }